[
  {
    "id": "aa1b2c3d-4e5f-6a7b-8c9d-0e1f2a3b4c5d",
    "text": "Life is a journey, not a destination.",
    "author": "Mark Twain",
    "category": "life",
    "language": "EN"
  },
  {
    "id": "889d0e1f-2a3b-4c5d-6e7f-8a9b0c1d2e3f",
    "text": "The greatest things in life aren't things.",
    "author": "Mark Twain",
    "category": "life",
    "language": "EN"
  },
  {
    "id": "ee4d5e6f-7g8h-9i0j-1k2l-3m4n5o6p7q8r",
    "text": "Nothing ventured, nothing gained.",
    "author": "Mark Twain",
    "category": "courage",
    "language": "EN"
  },
  {
    "id": "662l3m4n-5o6p-7q8r-9s0t-1u2v3w4x5y6z",
    "text": "Life is too short for someday.",
    "author": "Mark Twain",
    "category": "life",
    "language": "EN"
  },
  {
    "id": "113w4x5y-6z7a-8b9c-0d1e-2f3g4h5i6j7k",
    "text": "Life is short, but it is wide.",
    "author": "Mark Twain",
    "category": "life",
    "language": "EN"
  },
  {
    "id": "880d1e2f-3g4h-5i6j-7k8l-9m0n1o2p3q4r",
    "text": "Life is what happens when you're busy making other plans.",
    "author": "Mark Twain",
    "category": "life",
    "language": "EN"
  }
]
